{
  "name": "agora-frontend",
  "version": "1.0.0",
  "description": "React App of Agora",
  "private": true,
  "scripts": {
    "start": "node server/index.js",
    "start-dev": "cross-env NODE_ENV=development webpack serve --open",
    "build": "cross-env NODE_ENV=production webpack",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint --fix src --ext .js,.jsx",
    "test": "echo \"Error: no test specified\" && exit 1",
    "analyze": "source-map-explorer public/js/*.js",
    "build-css": "tailwindcss build -i src/styles/styles.css -o public/styles.css",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agora-jam/agora-frontend.git"
  },
  "author": "Agora",
  "bugs": {
    "url": "https://github.com/agora-jam/agora-frontend/issues"
  },
  "homepage": "https://github.com/agora-jam/agora-frontend",
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "axios": "^0.21.1",
    "daisyui": "1.16.2",
    "ethers": "^5.5.1",
    "express": "^4.17.1",
    "html-react-parser": "^1.2.8",
    "ipfs-http-client": "^54.0.0",
    "morgan": "^1.10.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "web3.storage": "^3.3.3",
    "web3modal": "^1.9.4",
    "zustand": "^3.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.0.2",
    "prettier": "^2.3.2",
    "source-map-explorer": "^2.5.2",
    "style-loader": "^3.2.1",
    "tailwindcss": "^2.2.19",
    "webpack": "^5.41.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.5.0"
  }
}
